[package]
name = "suricata"
version = "@PACKAGE_VERSION@"

[workspace]
members = [
    "dhcp-parser",
]

[lib]
crate-type = ["staticlib"]
path = "@e_rustdir@/src/lib.rs"

[profile.release]
debug = true

[features]
lua = []
strict = []
debug = []

[dependencies]
dhcp-parser = { path = "./dhcp-parser" }
nom = "4.2"
crc = "1.8"
der-parser = "1.1"
kerberos-parser = "0.2"

ntp-parser = "0.3"
ipsec-parser = "0.4"
snmp-parser = "0.3.0"
